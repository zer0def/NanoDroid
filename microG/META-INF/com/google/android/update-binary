#!/sbin/sh

umask 022
OUTFD="${2}"
ZIP="${3}"

MODID=NanoDroid_microG

mkdir -p /dev/tmp
unzip -oq "${ZIP}" CommonInstaller -d "/dev/tmp"
if [ ! -f /dev/tmp/CommonInstaller ]; then
	echo " !! failed to load Common Installer"
	exit 1
else
	source "/dev/tmp/CommonInstaller"
fi

setup_installer 1 1

install_nanodroidscripts
install_microg
set_progress 0.6

install_mapsv1
install_store
set_progress 0.9

nanodroid_finalize
set_progress 1.0

exit 0
